<p id="notice"><%= notice %></p>

<h1>Listing Location Options</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_options.each do |location_option| %>
      <tr>
        <td><%= link_to 'Show', location_option %></td>
        <td><%= link_to 'Edit', edit_location_option_path(location_option) %></td>
        <td><%= link_to 'Destroy', location_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location option', new_location_option_path %>
