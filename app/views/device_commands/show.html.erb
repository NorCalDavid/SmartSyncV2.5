<%- model_class = DeviceCommand -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:device_id) %></dt>
      <dd><pre class="prettyprint"><%= @device_command.device_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:comamnd_id) %></dt>
      <dd><pre class="prettyprint"><%= @device_command.comamnd_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:published) %></dt>
      <dd><pre class="prettyprint"><%= @device_command.published %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:executed_last) %></dt>
      <dd><pre class="prettyprint"><%= @device_command.executed_last %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:event_condition_id) %></dt>
      <dd><pre class="prettyprint"><%= @device_command.event_condition_id %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              device_commands_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_device_command_path(@device_command), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              device_command_path(@device_command),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>