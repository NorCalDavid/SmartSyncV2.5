<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Favorite</th>
      <th>Executed count</th>
      <th>Default</th>
      <th>0,</th>
      <th>Null</th>
      <th>False</th>
      <th>Executed last</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.description %></td>
        <td><%= event.favorite %></td>
        <td><%= event.executed_count %></td>
        <td><%= event.default %></td>
        <td><%= event.0, %></td>
        <td><%= event.null %></td>
        <td><%= event.false %></td>
        <td><%= event.executed_last %></td>
        <td><%= event.status %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
