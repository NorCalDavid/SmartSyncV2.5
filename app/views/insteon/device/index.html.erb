<div class="main-content">
  <div class="pageheader">
    <h1>Insteon Devices</h1>
    <p class="description">Manage Your Insteon Devices</p>
    <div class="breadcrumb-wrapper hidden-xs">
      <span class="label">You are here:</span>
      <ol class="breadcrumb">
          <li class="active">All Insteon Devices</li>
      </ol>
    </div>
  </div>

  <div class='container-body'>
    <div class="row padding-all-md">
      <div class="col-md-12">
        <div class="panel panel-primary">

          <div class="panel-heading">
            <%= link_to dashboard_path, class: 'btn btn-default embossed' do %>
            <span class="glyphicon glyphicon-plus"></span>
              Dashboard
            <% end %>
          </div>

          <div class="panel-body">
            <div class="table-responsive">
              <table class="table table-striped table-bordered table-hover">
                <thead>
                  <tr>
                    <th class="text-center">House ID</th>
                    <th class="text-center">Device ID</th>
                    <th class="text-center">Device Name</th>
                    <th class="text-center">Favorite?</th>
                    <th class="text-center">On Time</th>
                    <th class="text-center">Off Time</th>
                    <th class="text-center">Timer Enabled?</th>
                    <th class="text-center">Dim Level</th>
                    <th class="text-center">Ramp Rate</th>
                    <th class="text-center">LED Level</th>
                    <th class="text-center">Group</th>
                    <th class="text-center">Insteon ID</th>
                    <th class="text-center">Device Type</th>
                    <th class="text-center">Category</th>
                    <th class="text-center">Sub Category</th>
                    <th class="text-center">Product Type</th>
                    <th class="text-center">Manufacturer</th>
                    <th class="text-center">Serial Number</th>
                    <th class="text-center">Firmware Version</th>
                    <th class="text-center"></th>
                  </tr>
                </thead>
                <tbody>
                  <% @insteon_devices.each do |device| %>
                    <tr>
                      <td class="text-center"><%= device['HouseID'] %></td>
                      <td class="text-center"><%= device['DeviceID'] %></td>
                      <td class="text-center"><%= device['DeviceName'] %></td>
                      <td class="text-center"><%= device['Favorite'] %></td>
                      <td class="text-center"><%= device['OnTime'] %></td>
                      <td class="text-center"><%= device['OffTime'] %></td>
                      <td class="text-center"><%= device['TimerEnabled'] %></td>
                      <td class="text-center"><%= device['DimLevel'] %></td>
                      <td class="text-center"><%= device['RampRate'] %></td>
                      <td class="text-center"><%= device['LEDLevel'] %></td>
                      <td class="text-center"><%= device['Group'] %></td>
                      <td class="text-center"><%= device['InsteonID'] %></td>
                      <td class="text-center"><%= device['DeviceType'] %></td>
                      <td class="text-center"><%= device['DevCat'] %></td>
                      <td class="text-center"><%= device['SubCat'] %></td>
                      <td class="text-center"><%= device['ProductType'] %></td>
                      <td class="text-center"><%= device['Manufacturer'] %></td>
                      <td class="text-center"><%= device['SerialNumber'] %></td>
                      <td class="text-center"><%= device['FirmwareVersion'] %></td>
                      <td class="text-center">
                        
                      </td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div> 
        </div>
      </div>   
    </div>
  </div> 
</div> 
