<div class="main-content">
  <div class="pageheader">
    <h1>Insteon Houses</h1>
    <p class="description">Manage Your Insteon Houses</p>
    <div class="breadcrumb-wrapper hidden-xs">
      <span class="label">You are here:</span>
      <ol class="breadcrumb">
          <li class="active">All Insteon Houses</li>
      </ol>
    </div>
  </div>

  <div class='container-body'>
    <div class="row padding-all-md">
      <div class="col-md-12">
        <div class="panel panel-primary">

          <div class="panel-heading">
            <%= link_to dashboard_path, class: 'btn btn-default embossed' do %>
            <span class="glyphicon glyphicon-plus"></span>
              Dashboard
            <% end %>
          </div>

          <div class="panel-body">
            <div class="table-responsive">
              <table class="table table-striped table-bordered table-hover">
                <thead>
                  <tr>
                    <th class="text-center">House ID</th>
                    <th class="text-center">Name</th>
                    <th class="text-center">City</th>
                    <th class="text-center">Hub ID</th>
                    <th class="text-center">Hub Type</th>
                    <th class="text-center">Hub Username</th>
                    <th class="text-center">Hub Password</th>
                    <th class="text-center">IP</th>
                    <th class="text-center">Port</th>
                    <th class="text-center">Gateway</th>
                    <th class="text-center">DHCP?</th>
                    <th class="text-center">Mac Address</th>
                    <th class="text-center">Bin Version</th>
                    <th class="text-center">PLM Version</th>
                    <th class="text-center">Firmware</th>
                    <th class="text-center">Daylight Savings?</th>
                    <th class="text-center">Connected</th>
                    <th class="text-center"></th>
                  </tr>
                </thead>
                <tbody>
                  <% @insteon_houses.each do |house| %>
                    <tr>
                      <td class="text-center"><%= house['HouseID'] %></td>
                      <td class="text-center"><%= house['HouseName'] %></td>
                      <td class="text-center"><%= house['City'] %></td>
                      <td class="text-center"><%= house['InsteonHubID'] %></td>
                      <td class="text-center"><%= house['HubType'] %></td>
                      <td class="text-center"><%= house['HubUsername'] %></td>
                      <td class="text-center"><%= house['HubPassword'] %></td>
                      <td class="text-center"><%= house['IP'] %></td>
                      <td class="text-center"><%= house['Port'] %></td>
                      <td class="text-center"><%= house['Gateway'] %></td>
                      <td class="text-center"><%= house['DHCP'] %></td>
                      <td class="text-center"><%= house['Mac'] %></td>
                      <td class="text-center"><%= house['BinVer'] %></td>
                      <td class="text-center"><%= house['PLMVer'] %></td>
                      <td class="text-center"><%= house['FirmwareVer'] %></td>
                      <td class="text-center"><%= house['DaylightSavings'] %></td>
                      <td class="text-center"><%= house['HubReachable'] %></td>
                      <td class="text-center">
                        
                      </td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div> 
        </div>
      </div>   
    </div>
  </div> 
</div> 
